set term post
set key left box
set grid
set style data linespoints
set title "Approximation t = 0.0"
set output 'plot0.eps'
plot 'plot0.dat' using 1:2 index 0 title "t = 0.0" pt 1 lc 1
set title "Approximation t = 0.25"
set output 'plot1.eps'
plot 'plot1.dat' using 1:2 index 0 title "t = 0.25" pt 1 lc 1

